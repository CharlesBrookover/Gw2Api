{
  "name":              "landfill/gw2api",
  "description":       "Library to handle Arenanet's Guild Wars 2 API",
  "minimum-stability": "stable",
  "license":           "MIT",
  "authors":           [
    {
      "name":  "Charles Brookover",
      "email": "charlesbrookover@gmail.com"
    }
  ],
  "require":           {
    "php":               ">=8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev":       {
    "phpunit/phpunit":           "^9.5",
    "phpstan/phpstan":           "^1.5",
    "friendsofphp/php-cs-fixer": "^v3.8.0"
  },
  "autoload":          {
    "psr-4": {
      "Landfill\\Gw2Api\\": "src"
    }
  },
  "autoload-dev":      {
    "psr-4": {
      "Landfill\\Gw2Api\\Tests\\": "tests"
    }
  }
}
